package test;

/**
 * @author nimrodd
 *
 */
public class Plus extends BinaryExpression {
    public Plus(Expression num1, Expression num2)
    {
        super(num1,num2);
    }
	/**
	 * @return 
	 * 
	 */
	public double Plus() {
		return getLeft().calculate() + getRight().calculate()
	}

}
